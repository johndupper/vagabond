<button><%= link_to 'Back', cities_path %></button>

<% @posts.each do |post| %>
    <h3><%= post.title %> (<%= post.id %>)</h3>
    <p>Description:<%= post.description %></p>
    <p>Experience: work, live, or play?: <%= post.experience %></p>
    <p>USER: <%= post.user %></p>
    <p>Photo: <%=image_tag(post.photo_url) %></p>
    <p><%= post.created_at %></p>
<% end %>
