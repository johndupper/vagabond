<ul>
  <h1>Start Exploring</h1>

<div class="city-container">
  <ul>
    <% @cities.each do |city| %>
        <li>
          <h1>
            <a href="/cities/<%= city.id %>">
              <%= city.city_name %></a>
			</h1>
			<%=image_tag(city.city_photo_url) %>
			<p><%= city.description %></p>
    </li>
  <% end %>
</ul>
</div>	

